{
  "name": "test1",
  "version": "0.1.4",
  "description": "A Vue spa project",
  "scripts": {
    "test": "jest",
    "release": "npm run build && npm run smoke && standard-version",
    "lint": "eslint --fix --ext .js --ext .vue src",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js",
    "start": "cross-env npm run build && cross-env node build/webpack.start.js",
    "build": "cross-env NODE_ENV=production ENVIRONMENT=prod webpack --config ./build/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production ENVIRONMENT=prod webpack --config ./build/webpack.prod.js",
    "build:sit": "cross-env NODE_ENV=production ENVIRONMENT=sit webpack --config ./build/webpack.prod.js",
    "build:uat": "cross-env NODE_ENV=production ENVIRONMENT=uat webpack --config ./build/webpack.prod.js",
    "build:pre": "cross-env NODE_ENV=production ENVIRONMENT=pre webpack --config ./build/webpack.prod.js",
    "analyzer": "cross-env NODE_ENV=production webpack --config ./build/webpack.analyzer.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackdous/template-sap.git"
  },
  "keywords": [
    "vue",
    "webpack",
    "teamplete",
    "ssr",
    "sap",
    "multipage"
  ],
  "author": "zhangjingguo_19080088 <zhangjingguo@shinho.net.cn>",
